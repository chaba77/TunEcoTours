<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="448.0" prefWidth="704.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controles.AdminController">
    <TableView fx:id="usersTable" style="-fx-background-color: white; -fx-border-color: transparent; -fx-padding: 5px;" stylesheets="@button.css">
        <columns>
            <TableColumn fx:id="idColumn" text="ID" />
            <TableColumn fx:id="emailColumn" text="Email" />
            <TableColumn fx:id="rolesColumn" text="Roles" />
            <TableColumn fx:id="verifiedColumn" text="Verified" />
            <TableColumn fx:id="actionsColumn" text="Actions" />
        </columns>
    </TableView>
   <Button mnemonicParsing="false" onAction="#logout" prefHeight="50.0" prefWidth="98.0" stylesheets="@button.css" text="log out" />
</VBox>
